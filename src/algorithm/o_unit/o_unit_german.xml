<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="o_unit Algorithmus" type="constructive" call="o_unit" code="external" help_file="algorithm/o_unit_german.html">
  <heuristic>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta />
      <gamma objective="Sum_Ci"/>
    </problem>
  </heuristic>
  <exact>
    <problem>
      <alpha env="O"/>
      <beta processing_times="unit"/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta processing_times="unit"/>
      <gamma objective="Sum_Ci"/>
    </problem>
  </exact>
  <alg_controls>
    <integer name="ASTERN" caption="LR Parameter" default="-1"/>
    <integer name="SEED" caption="Zufalls-Seed" default="1234567890"/>
    <choice name="SORT" caption="Vorsortierungsregel">
	<item name="LPT"/>
	<item name="SPT"/>
	<item name="MINPTMIN"/>
	<item name="MINPTMAX"/>
	<item name="MAXPTMIN"/>
	<item name="MAXPTMAX"/>
	<item name="RAND"/>
    </choice>
  </alg_controls>
</algorithm>
