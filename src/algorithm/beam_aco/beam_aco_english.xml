<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="Beam Ant Colony" type="constructive" call="beam_aco" code="external" help_file="algorithm/beamaco.html">
  <heuristic>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
  </heuristic>
  <alg_controls>
    <integer name="BEAM_WIDTH" caption="Beamweite" default="100"/>
    <real name="LOWER_BOUND" caption="Untere Schranke" default="0"/>
    <real name="UPPER_BOUND" caption="Obere Schranke" default="1e+06"/>
    <choice name="EXTENSION_STRATEGY" caption="Bestimmung der Beam Extensions">
      <item name="MED"/>
      <item name="LDS"/>
      <item name="FIXED"/>
    </choice>
    <integer name="FIXED_KEXT" caption="Anzahl der Beam-Extensions" default="10"/>
    <choice name="APPEND_STRATEGY" caption="Strategie der Auswahl der Extensions">
      <item name="ORIGINAL"/>
      <item name="SORTED"/>
      </choice>
    <choice name="PRE_SELECTION" caption="Vorauswahl der Erweiterungen">
      <item name="NONE"/>
      <item name="ACTIVE"/>
     </choice>
    <integer name="STEPS" caption="Anzahl Durchlaeufe" default="50"/>
    <real name="CONVERGENCE_FACTOR" caption="Konvergenzfaktor fuer Neustart" default="0.98"/>
    <real name="EVAPORATION_RATE" caption="evaporationRate" default="0.3"/>
    <real name="WEIGHT_EST" caption="Einfluss der Earliest Completion Time" default="1"/>
  <real name="WEIGHT_TIJ" caption="Einfluss der Pheromone Werte" default="1"/>
  <real name="WEIGHT_RAND" caption="Einfluss des Zufalls" default="1"/>

  </alg_controls>
</algorithm>
