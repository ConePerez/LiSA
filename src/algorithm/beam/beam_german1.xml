<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="Beam Search" type="constructive" call="beam" code="external" help_file="algorithm/beam.html">
  <heuristic>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="F"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="F"/>
      <beta release_times="yes"/>
      <gamma objective="Cmax"/>
    </problem>
  </heuristic>
  <alg_controls>
    <integer name="K_BRANCHES" caption="Anzahl zu verfolgender Zweige" default="5"/>
    <choice name="INS_ORDER" caption="Einfuegereihenfolge">
      <item name="MACHINEWISE"/>
      <item name="DIAGONAL1"/>
      <item name="DIAGONAL2"/>
      <item name="RANDOM"/>
      <item name="LPT"/>
						<item name="ECT"/>
						<item name="SPT"/>
    </choice>
    <choice name="INS_METHOD" caption="Auswahlmethode">
      <item name="INSERT1"/>
      <item name="INSERT2"/>
    </choice>
    <choice name="CRITERION" caption="Auswahlkriterium">
      <item name="CMAX"/>
      <item name="CLAST"/>
    </choice>
  </alg_controls>
</algorithm>
