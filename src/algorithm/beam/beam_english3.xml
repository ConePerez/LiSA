<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="Beam Search" type="constructive" call="beam" code="external" help_file="algorithm/beam.html">
  <heuristic>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ci"/>
    </problem>
  </heuristic>
  <exact>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
  </exact>
  <alg_controls>
    <integer name="K_BRANCHES" caption="Beamwidth" default="5"/>
    <choice name="INS_ORDER" caption="Insertion order">
      <item name="MACHINEWISE"/>
      <item name="DIAGONAL"/>
      <item name="QUEEN_SWEEP"/>
      <item name="RANDOM"/>
      <item name="LPT"/>
      <item name="ECT"/>
      <item name="SPT"/>
      <item name="ATTATCH"/>
    </choice>
    <choice name="INS_METHOD" caption="Choice method">
      <item name="INSERT1"/>
      <item name="INSERT2"/>
    </choice>
    <choice name="ATTATCH_WHAT" caption="Attatch at">
	<item name="Machines"/>
	<item name="Jobs"/>
	<item name="Machines+Jobs"/>
    </choice>
  </alg_controls>
</algorithm>
