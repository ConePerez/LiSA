<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="Smith Rule" type="constructive" call="dispatch" code="external" help_file="algorithm/dispatch.html">
  <heuristic>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Lmax"/>
    </problem>
  </heuristic>
  <exact>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
