<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="GenAlg" type="iterative" call="ga" code="external" help_file="algorithm/ga.html">
  <heuristic>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
  </heuristic>
  <alg_controls>
    <integer name="POP_SIZE" caption="Populationsgröße" default="50"/>
    <integer name="NUM_GEN"  caption="Anzahl Generationen" default="50"/>
    <real    name="M_PROB"   caption="Mutationswahrscheinlichkeit" default="0.1"/>
    <real    name="S_PROB"   caption="Selectionswahrscheinlichkeit" default="0.3"/>
    <integer name="SEED"     caption="Zufalls-Seed" default="1234567890"/>
    <integer name="IMPR_STEPS"  caption="Anzahl lokaler Suchschritte" default="10"/>
    <choice name="L_IMPR" caption="Lokale Suche">
      <item name="CR_TST"/>
      <item name="API"/>
      <item name="SHIFT"/>
      <item name="PI"/>
      <item name="CR_API"/>
      <item name="BL_API"/>
      <item name="CR_SHIFT"/>
      <item name="BL_SHIFT"/>
      <item name="3_CR"/>
      <item name="(disabled)"/>
    </choice>
  </alg_controls>
</algorithm>
