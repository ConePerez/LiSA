<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="Iterative Improvement" type="iterative" call="nb_iter" code="external" help_file="algorithm/nb_iter.html">
  <heuristic>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Lmax"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_wiCi"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_Ui"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_wiUi"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_Ti"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta/>
      <gamma objective="Sum_wiTi"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Lmax"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_wiCi"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ui"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_wiUi"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ti"/>
    </problem>
    <problem>
      <alpha env="1"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_wiTi"/>
    </problem>
  </heuristic>
  <alg_controls>
    <integer name="STEPS" caption="Anzahl der Loesungen" default="5000"/>
    <integer name="NUMB_STUCKS" caption="Abbruch nach Stillstaenden (Abbruchkriterium)" default="10000"/>
    <real name="ABORT_BOUND" caption="Abbruchschranke (Abbruchkriterium)" default="0"/>
    <choice name="NGBH" caption="Nachbarschaft">
      <item name="1_API"/>
      <item name="SHIFT"/>
      <item name="PI"/>
    </choice>
    <fixed name="METHOD"  value="IterativeImprovement"/>
    <choice name="TYPE" caption="Erzeugemodus">
      <item name="ENUM"/>
      <item name="RAND"/>
    </choice>
  </alg_controls>
</algorithm>
